{
	"name": "pose",
	"displayName": "Pose Viewer",
	"description": "visualize .pose files right in your vscode",
	"version": "0.0.3",
	"icon": "images/pose.png",
	"publisher": "bipinkrish",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/bipinkrish/vscode-pose"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "poseViewer.pose",
				"displayName": "Pose Viewer",
				"selector": [
					{
						"filenamePattern": "*.pose"
					}
				],
				"priority": "default"
			}
		],
		"languages": [
			{
				"id": "pose",
				"aliases": [
					"Pose"
				],
				"extensions": [
					".pose"
				],
				"icon": {
					"light": "images/pose.png",
					"dark": "images/pose.png"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/node": "^18",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^7.14.0",
		"@typescript-eslint/parser": "^7.14.0",
		"eslint": "^8.26.0",
		"typescript": "^5.5.2"
	}
}