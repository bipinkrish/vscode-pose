{
  "name": "pose",
  "displayName": "Pose Viewer",
  "description": "visualize .pose files with flexible controls right in your vscode",
  "version": "0.1.1",
  "icon": "images/pose.png",
  "publisher": "bipinkrish",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bipinkrish/vscode-pose"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "poseViewer.pose",
        "displayName": "Pose Viewer",
        "selector": [
          {
            "filenamePattern": "*.pose"
          }
        ],
        "priority": "default"
      }
    ],
    "languages": [
      {
        "id": "pose",
        "aliases": [
          "Pose"
        ],
        "extensions": [
          ".pose"
        ],
        "icon": {
          "light": "images/pose.png",
          "dark": "images/pose.png"
        }
      }
    ]
  },
  "scripts": {
    "package:prepublish": "rm -rf out && npm run compile",
    "compile": "tsc -p ./ && webpack --mode production",
    "watch": "tsc -w -p ./ & webpack --mode development --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "package": "npm run package:prepublish && vsce package"
  },
  "devDependencies": {
    "@types/node": "^18",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^7.14.0",
    "@typescript-eslint/parser": "^7.14.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.26.0",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.2",
    "webpack": "^5.97.1"
  },
  "dependencies": {
    "@stencil/core": "^4.23.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vscode/vsce": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-pose-viewer": "^1.0.4",
    "webpack-cli": "^6.0.1"
  }
}
